{
    "name": "@wavechat/rtc",
    "version": "1.0.0",
    "description": "RTC server for wavechat",
    "main": "dist/src/index.js",
    "author": "FourLineCode",
    "license": "MIT",
    "homepage": "https://github.com/FourLineCode/wavechat#readme",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "build": "tsc",
        "start": "NODE_PATH=dist node dist/src/index.js",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf node_modules && pnpm clean:cache",
        "clean:cache": "rm -rf dist && rm -rf .turbo"
    },
    "dependencies": {
        "cookie": "^0.4.2",
        "dotenv": "^16.0.0",
        "fastify": "^3.27.2",
        "fastify-cookie": "^5.5.0",
        "fastify-cors": "^6.0.2",
        "fastify-helmet": "^7.0.1",
        "fastify-socket.io": "^3.0.0",
        "graphql": "^16.3.0",
        "graphql-request": "^4.0.0",
        "ioredis": "^4.28.4",
        "jsonwebtoken": "^8.5.1",
        "socket.io": "^4.4.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/cookie": "^0.4.1",
        "@types/ioredis": "^4.28.8",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.18",
        "@types/uuid": "^8.3.4",
        "@wavechat/shared": "workspace:*",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.5.5"
    }
}
